{
  "name": "wallride-match",
  "version": "1.0.0",
  "description": "Matching and filtering JS objects (with TypeScript typings)",
  "main": "./build/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "pretest": "rm -rf ./tests/build && cd ./tests && gulp && cd .. | bunyan --color -o short",
